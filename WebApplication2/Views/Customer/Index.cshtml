@{
}
@model List<WebApplication2.Models.Customer>
<h2>Customer</h2>
<table class="table">
    <thead>
        <tr>
        <th>CIVIL ID</th>
        <th>name</th>
        <th>phone</th>
        </tr>

    </thead>
    <tbody>
        
        @foreach (var c in Model)
        {
            <tr>
                <td>@c.CivilId</td>
                <td>@c.Name</td>
                <td>@c.Phone</td>
            </tr>
        }
    </tbody>
</table>

